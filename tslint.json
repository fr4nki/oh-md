{
    "defaultSeverity": "warning",
    "extends": "tslint-config-airbnb",
    "rules": {
        "indent": [true, "spaces", 4],
        "ter-indent": [true, 4],
        "import-name": false,
        "quotemark": [true, "single", "jsx-single"],
        "trailing-comma": true,
        "no-boolean-literal-compare": true
    }
}
